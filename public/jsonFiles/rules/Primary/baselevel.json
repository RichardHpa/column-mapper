	"NetContentValue": {
		"Name": "NetContentValue",
		"DisplayName": "Net Content",
		"IsExportable": true,
		"ExportOrder": 5,
		"Placeholder": "Net Content",
		"IsPrimary": true,
		"RequiredInputType": 0,
		"HelpMessage": "",
		"Parameters": ["NetContentUom"],
		"Conditions": {
			"GDSN": {
				"ViewConditions": [
					{
						"IsVisible": "true",
						"IsEnabled": "true"
					}
				],
				"ValidationConditions": [
					{
						"IsError": true,
						"IsValid": "!isnull(input)",
						"ValidationMessage": "This is required."
					},
					{
						"IsError": true,
						"IsValid": "isnull(input) || NetContentUom != \"EA\" || getdecimalpoint(input) <= 0",
						"ValidationMessage": "Cannot have decimal places."
					},
					{
						"IsError": true,
						"IsValid": "isnull(input) || NetContentUom != \"GRM\" || getdecimalpoint(input) <= 2",
						"ValidationMessage": "No more than 2 decimal places."
					},
					{
						"IsError": true,
						"IsValid": "isnull(input) || NetContentUom != \"KGM\" || getdecimalpoint(input) <= 3",
						"ValidationMessage": "No more than 3 decimal places."
					},
					{
						"IsError": true,
						"IsValid": "isnull(input) || NetContentUom != \"LTR\" || getdecimalpoint(input) <= 3",
						"ValidationMessage": "No more than 3 decimal places."
					},
					{
						"IsError": true,
						"IsValid": "isnull(input) || NetContentUom != \"MLT\" || getdecimalpoint(input) <= 3",
						"ValidationMessage": "No more than 3 decimal places."
					},
					{
						"IsError": true,
						"IsValid": "isnull(input) || NetContentUom != \"MLT\" || getdecimalpoint(input) <= 3",
						"ValidationMessage": "No more than 3 decimal places."
					},
					{
						"IsError": true,
						"IsValid": "isnull(input) || NetContentUom != \"GRM\" || input < 100000",
						"ValidationMessage": "Must be less than 100000."
					},
					{
						"IsError": true,
						"IsValid": "isnull(input) || NetContentUom != \"KGM\" || input < 100",
						"ValidationMessage": "Must be less than 100."
					}
				]
			}
		}
	},
