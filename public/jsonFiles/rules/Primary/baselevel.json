	"Description": {
		"Name": "Description",
		"DisplayName": "Trade Item Description",
		"IsExportable": true,
		"ExportOrder": 8,
		"Placeholder": "Trade Item Description",
		"IsPrimary": true,
		"RequiredInputType": 1,
		"HelpMessage": "",
		"Parameters": [
			"NetContentValue",
			"VariantDescription",
			"Brand",
			"SubBrand"
		],
		"Conditions": {
			"GDSN": {
				"ViewConditions": [
					{
						"IsVisible": "true",
						"IsEnabled": "true"
					}
				],
				"ValidationConditions": [
					{
						"IsError": true,
						"IsValid": "!isnull(input)",
						"ValidationMessage": "This is required."
					},
					{
						"IsError": true,
						"IsValid": "!input.Contains(\",\") || isnull(input)",
						"ValidationMessage": "Description must not contain \",\"."
					},
					{
						"IsError": false,
						"IsValid": "isnull(input) || isnull(NetContentValue) || NetContentValue == 1.0 || (\" \"+input+\" \").ToUpper().Contains((\" \"+(NetContentValue)).ToUpper())",
						"ValidationMessage": "Description should contain \"{NetContentValue}\"."
					},
					{
						"IsError": false,
						"IsValid": "isnull(input) || isnull(VariantDescription) ||(\" \"+input+\" \").ToUpper().Contains((\" \"+VariantDescription+\" \").ToUpper())",
						"ValidationMessage": "Description should contain \"{VariantDescription}\"."
					},
					{
						"IsError": false,
						"IsValid": "isnull(input) || isnull(SubBrand) ||(\" \"+input+\" \").ToUpper().Contains((\" \"+SubBrand+\" \").ToUpper())",
						"ValidationMessage": "Description should contain \"{SubBrand}\"."
					},
					{
						"IsError": false,
						"IsValid": "isnull(input) || isnull(Brand) ||(\" \"+input+\" \").ToUpper().Contains((\" \"+Brand+\" \").ToUpper())",
						"ValidationMessage": "Description should contain \"{Brand}\"."
					}
				]
			}
		}
	},
