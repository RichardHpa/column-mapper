	"PackagingMaterialCompositionQuantityBaseUnit": {
		"Name": "PackagingMaterialCompositionQuantityBaseUnit",
		"Conditions": {
			"Foodstuffs": {
				"ViewConditions": [
					{
						"IsVisible": "true",
						"IsEnabled": "true"
					}
				],
				"ValidationConditions": [
					{
						"IsError": true,
						"IsValid": "isnull(input) || getdecimalpoint(input) <= 3",
						"ValidationMessage": "No more than 3 decimal places"
					},
					{
						"IsError": true,
						"IsValid": "!isnull(input)",
						"ValidationMessage": "This is a mandatory field"
					},
					{
						"IsError": true,
						"IsValid": "isnull(input) || input != 0 && Packaging_CalculatedMaterialWeightBaseUnit != 0 || isnull(PackagingTypeCodeBaseUnit) || PackagingTypeCodeBaseUnit == \"NE\"",
						"ValidationMessage": "Can only be 0kg if the item is not packed, adjust your gross weight to account for the weight of the packaging"
					},
					{
						"IsError": true,
						"IsValid": "PackagingTypeCodeBaseUnit == \"NE\" && input == 0 || PackagingTypeCodeBaseUnit != \"NE\"",
						"ValidationMessage": "You have selected 'Not Packed', the packaging material weight must be 0kg"
					},
					{
						"IsError": true,
						"IsValid": "isnull(input) || Packaging_CalculatedMaterialWeightBaseUnit == 0 || isnull(Packaging_CalculatedMaterialWeightBaseUnit) || isnull(PackagingTypeCodeBaseUnit) || input > (Packaging_CalculatedMaterialWeightBaseUnit * 0.9) && input < (Packaging_CalculatedMaterialWeightBaseUnit * 1.1)",
						"ValidationMessage": "Should be approximately {Packaging_CalculatedMaterialWeightBaseUnit}kg"
					}
				]
			}
		}
	},
