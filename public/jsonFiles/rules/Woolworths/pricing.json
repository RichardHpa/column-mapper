    "WW_StartDate": {
        "Name": "WW_StartDate",
        "Placeholder": "",
        "IsPrimary": true,
        "RequiredInputType": 3,
        "HelpMessage": "",
        "Parameters": [
            "WW_HasDeferredDeal",
            "WW_Reason",
            "WW_LastStartDate"
        ],
        "Conditions": {
            "Woolworths": {
                "ViewConditions": [
                    {
                        "IsVisible": "true",
                        "IsEnabled": "true"
                    }
                ],
                "ValidationConditions": [
                    {
                        "IsError": true,
                        "IsValid": "!isnull(input)",
                        "ValidationMessage": "This is required."
                    },
                    {
                        "IsError": true,
                        "IsValid": "isnull(input) || !WW_HasDeferredDeal || (WW_HasDeferredDeal && input.Value.DayOfWeek == DayOfWeek.Monday)",
                        "ValidationMessage": "Because you have Deferred Deal set, date must be set on Monday."
                    },
                    {
                        "IsError": false,
                        "IsValid": "isnull(WW_Reason) || !(WW_Reason == \"NI\" && input > now().Date)",
                        "ValidationMessage": "You have indicated this is a new item, therefore the date entered should not be a future date"
                    },
                    {
                        "IsError": true,
                        "IsValid": "isnull(input) || !WW_LastStartDate.HasValue || (input > now().AddDays(1).Date && input > WW_LastStartDate.Value.AddDays(1))",
                        "ValidationMessage": "Your previous price entry has Effective Start Date of {WW_LastStartDate}. Therefore, the new Effective Start Date must be greater than {WW_LastStartDate.Value.AddDays(2) > now().Date.AddDays(1) ? WW_LastStartDate.Value.AddDays(2) : now().Date.AddDays(1)}"
                    }
                ]
            }
        }
    }
